_crab_make_rf(NNN,SIDE,IRN,BIM,FFF): macro {
    kn1_crabNNNSIDEIRNBIM:=ahcrab_sideirnbim/fff*cosiirn;
    ks1_crabNNNSIDEIRNBIM:=avcrab_sideirnbim/fff*sinuirn;
    acfga.4NNNSIDEIRN.BIM: rfmultipole,l=0, freq=crabrf,
    knl:={on_crabirn*kn1_crabNNNSIDEIRNBIM,
          on_crabirn*kn2_crabNNNSIDEIRNBIM,
          on_crabirn*kn3_crabNNNSIDEIRNBIM,
          on_crabirn*kn4_crabNNNSIDEIRNBIM},
    ksl:={on_crabirn*ks1_crabNNNSIDEIRNBIM,
          on_crabirn*ks2_crabNNNSIDEIRNBIM,
          on_crabirn*ks3_crabNNNSIDEIRNBIM,
          on_crabirn*ks4_crabNNNSIDEIRNBIM},
    pnl:={0.25,pn2_crabNNNSIDEIRNBIM,pn3_crabNNNSIDEIRNBIM,pn4_crabNNNSIDEIRNBIM},
    psl:={0.25,ps2_crabNNNSIDEIRNBIM,ps3_crabNNNSIDEIRNBIM,ps4_crabNNNSIDEIRNBIM};
};

l_crab=0;

_crab_make_cc(nnn,side,irn,bim,fff): macro {
    vhcrabref_sideirnbim:=ahcrab_sideirnbim/fff*cosiirn*nrj*1e9/1e6;
    vvcrabref_sideirnbim:=avcrab_sideirnbim/fff*sinuirn*nrj*1e9/1e6;
    acfgah.4nnnsideirn.bim: crabcavity, l:=l_crab,
      volt:=on_crabirn*vhcrabref_sideirnbim*bv_aux, !implements reflect.py,
      lag:=phi_crab_sideirnbim*bv_aux + lagref_crab_bim * isnotb4, !implements reflect.py 3 sign changes s,x,z,
      freq=crabrf/1e6,tilt=0,rv3=1e9,rv4=1e9;
    acfgav.4nnnsideirn.bim: crabcavity, l:=l_crab,
      volt:=on_crabirn*vvcrabref_sideirnbim, ! 2 sign change s,z
      lag:=phi_crab_sideirnbim + lagref_crab_bim * isnotb4,
      freq:=crabrf/1e6,tilt=pi/2,rv3=1e9,rv4=1e9;
    print,text="making acfgah.4nnnsideirn.bim";
    print,text="making acfgav.4nnnsideirn.bim";
};

crab_make_cc(NNN,FFF): macro {
lagref_crab_b1 = 0;
lagref_crab_b2 = 0.5;
isnotb4 = (1 + bv_aux) / 2;
exec,_crab_make_cc(NNN,l,5,b1,FFF);
exec,_crab_make_cc(NNN,l,5,b2,FFF);
exec,_crab_make_cc(NNN,r,5,b1,FFF);
exec,_crab_make_cc(NNN,r,5,b2,FFF);
exec,_crab_make_cc(NNN,l,1,b1,FFF);
exec,_crab_make_cc(NNN,l,1,b2,FFF);
exec,_crab_make_cc(NNN,r,1,b1,FFF);
exec,_crab_make_cc(NNN,r,1,b2,FFF);
};

crab_make_rf(NNN,FFF): macro {
exec,_crab_make_rf(NNN,l,5,b1,FFF);
exec,_crab_make_rf(NNN,l,5,b2,FFF);
exec,_crab_make_rf(NNN,r,5,b1,FFF);
exec,_crab_make_rf(NNN,r,5,b2,FFF);
exec,_crab_make_rf(NNN,l,1,b1,FFF);
exec,_crab_make_rf(NNN,l,1,b2,FFF);
exec,_crab_make_rf(NNN,r,1,b1,FFF);
exec,_crab_make_rf(NNN,r,1,b2,FFF);
};

