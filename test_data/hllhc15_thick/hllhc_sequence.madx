crabrf:=hrf400/lhclength*clight;

crab_make_tkicker(NNN,FFF): macro {
omega_crabsc:=twopi*HRF400/LHCLENGTH;
z_crab:=0; ! positive means head of the bunch i.e. z=-cT and z==t(madx)

phi_crab_l1b1 := 0;
phi_crab_r1b1 := 0;
phi_crab_l1b2 := 0;
phi_crab_r1b2 := 0;
phi_crab_l5b1 := 0;
phi_crab_r5b1 := 0;
phi_crab_l5b2 := 0;
phi_crab_r5b2 := 0;

sin_crab_l1b1:=sin(-omega_crabsc*z_crab+twopi*phi_crab_l1b1);
sin_crab_r1b1:=sin(-omega_crabsc*z_crab+twopi*phi_crab_r1b1);
sin_crab_l1b2:=sin(-omega_crabsc*z_crab+twopi*phi_crab_l1b2);
sin_crab_r1b2:=sin(-omega_crabsc*z_crab+twopi*phi_crab_r1b2);
sin_crab_l5b1:=sin(-omega_crabsc*z_crab+twopi*phi_crab_l5b1);
sin_crab_r5b1:=sin(-omega_crabsc*z_crab+twopi*phi_crab_r5b1);
sin_crab_l5b2:=sin(-omega_crabsc*z_crab+twopi*phi_crab_l5b2);
sin_crab_r5b2:=sin(-omega_crabsc*z_crab+twopi*phi_crab_r5b2);

!sin_crab:=sin(omega_crabsc*z_crab);
cosi1:=cos(twopi*phi_IR1/360.);sinu1:=sin(twopi*phi_IR1/360.);
cosi5:=cos(twopi*phi_IR5/360.);sinu5:=sin(twopi*phi_IR5/360.);
on_crab1=0;on_crab5=0;
acfga.4NNNl1.b1: tkicker, l=0, HKICK:=on_crab1*ahcrab_l1b1/FFF*sin_crab_l1b1*cosi1,VKICK:=on_crab1*avcrab_l1b1/FFF*sin_crab_l1b1*sinu1*bv_aux, mech_sep=-0.194;
acfga.4NNNl1.b2: tkicker, l=0, HKICK:=on_crab1*ahcrab_l1b2/FFF*sin_crab_l1b2*cosi1,VKICK:=on_crab1*avcrab_l1b2/FFF*sin_crab_l1b2*sinu1*bv_aux, mech_sep= 0.194*bv_aux;
acfga.4NNNr1.b1: tkicker, l=0, HKICK:=on_crab1*ahcrab_r1b1/FFF*sin_crab_r1b1*cosi1,VKICK:=on_crab1*avcrab_r1b1/FFF*sin_crab_r1b1*sinu1*bv_aux, mech_sep= 0.194;
acfga.4NNNr1.b2: tkicker, l=0, HKICK:=on_crab1*ahcrab_r1b2/FFF*sin_crab_r1b2*cosi1,VKICK:=on_crab1*avcrab_r1b2/FFF*sin_crab_r1b2*sinu1*bv_aux, mech_sep=-0.194*bv_aux;
acfga.4NNNl5.b1: tkicker, l=0, HKICK:=on_crab5*ahcrab_l5b1/FFF*sin_crab_l5b1*cosi5,VKICK:=on_crab5*avcrab_l5b1/FFF*sin_crab_l5b1*sinu5*bv_aux, mech_sep=-0.194;
acfga.4NNNl5.b2: tkicker, l=0, HKICK:=on_crab5*ahcrab_l5b2/FFF*sin_crab_l5b2*cosi5,VKICK:=on_crab5*avcrab_l5b2/FFF*sin_crab_l5b2*sinu5*bv_aux, mech_sep= 0.194*bv_aux;
acfga.4NNNr5.b1: tkicker, l=0, HKICK:=on_crab5*ahcrab_r5b1/FFF*sin_crab_r5b1*cosi5,VKICK:=on_crab5*avcrab_r5b1/FFF*sin_crab_r5b1*sinu5*bv_aux, mech_sep= 0.194;
acfga.4NNNr5.b2: tkicker, l=0, HKICK:=on_crab5*ahcrab_r5b2/FFF*sin_crab_r5b2*cosi5,VKICK:=on_crab5*avcrab_r5b2/FFF*sin_crab_r5b2*sinu5*bv_aux, mech_sep=-0.194*bv_aux;
};

